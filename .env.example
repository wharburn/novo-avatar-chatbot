# Hume AI Configuration
# Get your API key and secret from https://platform.hume.ai/settings/keys

# Server-side credentials (required for access token generation)
HUME_API_KEY=your_hume_api_key_here
HUME_SECRET_KEY=your_hume_secret_key_here

# Hume EVI Config ID (client-side, must be prefixed with NEXT_PUBLIC_)
# Create an EVI configuration at https://platform.hume.ai/evi/configs
NEXT_PUBLIC_HUME_CONFIG_ID=your_evi_config_id_here

# App Configuration (optional)
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_ENV=development

# Redis Configuration (optional - for session tracking)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# Admin Configuration (optional)
ADMIN_PIN=your_admin_pin
